/*
requests INDEX/SEARCH
*/

#requests {
	margin-left: 15px;

	#map-canvas2{
		position: absolute;
		height: 580px;
		width: 50%;
		right: 0;
	}
	#request-collection {
		width: 48%;
		-moz-column-count: 2;
		-webkit-column-count: 2;
		column-count: 2;

		.request-index {
			border: 1px solid #f5a623;
			margin-bottom: 10px;
			display: inline-block;
		}
	}
}
/*
request new
*/
.request-title {
	margin-bottom: 40px;
	
	.request-listing {
		color: #f5a623;
		font-weight: 600;
	}
}

.request-container {
	background-color: #eaeaea;
	width: 95%;
	text-align: center;
	margin: auto;

}